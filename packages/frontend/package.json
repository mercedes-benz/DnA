  "config": {
    "title": "DnA@MBC App",
    "AppIcon": "./src/assets/appIcon/logo.png",
    "devServer": {
      "host": "0.0.0.0",
      "port": "9090",
      "https": false,
      "publicPath": "/"
    },
    "publicPath": "auto"
  },
  "scripts": {
    "test": "jest",
    "test:update": "jest --updateSnapshot",
    "start": "webpack serve --env build=dev",
    "start:docker": "webpack-dev-server --env build=dev --env ENV_FILE=.docker.env",
    "start:legacy": "webpack-dev-server --env build=dev --env legacy=true",
    "build": "node --max_old_space_size=4096 node_modules/.bin/webpack --env build=prod --env verbose=false",
    "build:legacy": "node --max_old_space_size=4096 node_modules/.bin/webpack --env build=prod --env legacy=true",
    "build:exposed": "node --max_old_space_size=4096 node_modules/.bin/webpack --env build=prod --env legacy=true --env exposed=true",
    "build:swidget": "node --max_old_space_size=4096 node_modules/.bin/webpack --env build=swidget --env legacy=true",
    "build:multi": "node --max_old_space_size=4096 node_modules/.bin/webpack --env build=multi",
    "build:docker": "node --max_old_space_size=4096 node_modules/.bin/webpack --env build=multi --env ENV_FILE=.docker.env",
    "clean": "rimraf dist && rimraf coverage",
    "build:win": "webpack --config ./webpack.config.js --env build=multi",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint src/**/*.{ts,tsx} --ignore-pattern \"/**/*.d.ts\"",
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mercedes-benz/DnA.git"
